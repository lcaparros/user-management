{
  "name": "@lcaparros/user-management",
  "version": "1.0.1",
  "description": "This is a JS module for adding basic user management to your ExpressJS project",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development node --watch --env-file=local.env index",
    "test:log": "NODE_ENV=test node --experimental-vm-modules --env-file=test.env node_modules/jest/bin/jest.js --runInBand --verbose",
    "test": "npm run test:log -- --silent",
    "test:watch": "npm run test -- --watch",
    "test:log:watch": "npm run test:log -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lcaparros/user-management.git"
  },
  "keywords": [
    "user-management",
    "credentials",
    "authentication"
  ],
  "author": "lcaparros",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lcaparros/user-management/issues"
  },
  "homepage": "https://github.com/lcaparros/user-management#readme",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "express": "4.19.2",
    "jsonwebtoken": "9.0.2",
    "mongoose": "8.4.1"
  },
  "devDependencies": {
    "jest": "29.7.0",
    "supertest": "7.0.0"
  },
  "directories": {
    "test": "tests"
  }
}
